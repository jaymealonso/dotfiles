SSH_ENV="$HOME/.ssh/agent-environment"

function start_agent {
    echo "Initialising new SSH agent..."
    /usr/bin/ssh-agent | sed 's/^echo/#echo/' > "${SSH_ENV}"
    echo succeeded
    chmod 600 "${SSH_ENV}"
    . "${SSH_ENV}" > /dev/null
    # /usr/bin/ssh-add;
    # /usr/bin/ssh-add ~/.ssh/id_rsa_personal;
    /usr/bin/ssh-add -D;
    /usr/bin/ssh-add ~/.ssh/id_moncler;
}

